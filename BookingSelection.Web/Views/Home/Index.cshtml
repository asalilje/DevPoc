@{
    ViewBag.Title = "BookingSelection";
}
@model BookingSelection.Web.Models.BookingModel

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}
<h3>Sök bokningar</h3>

@using(Html.BeginForm("Search", "Home")) {
    @Html.HiddenFor(model => model.DeviationId)
    @Html.DropDownList("BookingTypeId", new SelectList(Model.BookingTypes, "Id", "Name",  Model.BookingTypeId))
    <input type="submit" value="Sök" />
}



@ViewBag.Saved